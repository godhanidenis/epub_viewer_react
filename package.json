{
  "name": "epub_viewer",
  "version": "0.1.3",
  "description": "Epub viewer for React.js powered by Epub.js",
  "private": true,
  "main": "lib/modules/index.js",
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "react-scripts test --watchAll=false --passWithNoTests",
    "build": " react-scripts build",
    "test": "react-scripts test",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "clean": "rimraf lib",
    "precompile": "react-scripts test --watchAll=false --passWithNoTests && yarn clean",
    "compile": "babel src --out-dir lib --extensions .ts,.tsx",
    "postcompile": "cp -r ./src/types ./lib/types"
  },
  "dependencies": {
    "@types/react-pdf": "^5.7.2",
    "@types/react-sticky-el": "^1.0.3",
    "axios": "^0.27.2",
    "bulma": "^0.9.4",
    "bulma-helpers": "^0.4.2",
    "epubjs": "^0.3.93",
    "material-design-icons": "^3.0.1",
    "npm-check-updates": "^13.0.3",
    "pdf-viewer-reactjs": "^2.2.3",
    "pdfjs-dist": "^2.14.305",
    "rc-progress": "^3.3.3",
    "react-epub-viewer": "^0.2.0",
    "react-full-screen": "^1.1.0",
    "react-pdf": "^5.7.2",
    "react-router-dom": "^6.3.0",
    "react-toastify": "^9.0.1",
    "react-tooltip": "^4.2.21",
    "styled-icons": "^10.45.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.0",
    "@babel/plugin-proposal-optional-chaining": "^7.17.12",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@reduxjs/toolkit": "^1.8.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.36",
    "@types/pdf-viewer-reactjs": "^2.2.2",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-sticky": "^6.0.4",
    "@types/styled-components": "^5.1.25",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "gh-pages": "^4.0.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-redux": "^8.0.2",
    "react-scripts": "^5.0.1",
    "styled-components": "^5.3.5",
    "typescript": "^4.7.2"
  },
  "peerDependencies": {
    "react": ">=17.0.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "types": "lib/types/index.d.ts"
}
